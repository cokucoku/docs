body, html {
    font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif;-webkit-tap-highlight-color:rgba(0,0,0,0);
}

table{font-size: 14px;width:100%;display:table!important}
th{color: #999;font-weight: normal;}
th,td{word-break: break-all;}
.content p{color:#999;}
thead{background-color: #46bd87;font-weight:bold}
thead th{font-weight:bold;color:#fff}
.leeblock{border:solid 1px #eaecef;margin-top:1em;border-radius: 3px;position:relative;z-index:1}
.leesource{padding:20px;}
